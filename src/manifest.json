{
    "name": "AWS SSO Logout",
    "version": "0.0.0",
    "description": "",
    "manifest_version": 2,
    "permissions": [
        "cookies",
        "webRequest",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["index.js"],
        "persistent": true
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}